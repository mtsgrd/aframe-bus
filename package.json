    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "browserify": "^14.0.0",
    "browserify-derequire": "^0.9.4",
    "browserify-istanbul": "^2.0.0",
    "chai": "^3.5.0",
    "chai-shallow-deep-equal": "^1.4.0",
    "chalk": "^1.1.3",
